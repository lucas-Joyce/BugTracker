{
  "name": "bugtracker",
  "version": "1.0.0",
  "description": "bug tracking project",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start:client": "cd client && npm start",
    "start:server": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucas-Joyce/BugTracker.git"
  },
  "keywords": [
    "React",
    "Node",
    "Express",
    "MongoDB"
  ],
  "author": "Lucas Joyce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucas-Joyce/BugTracker/issues"
  },
  "homepage": "https://github.com/lucas-Joyce/BugTracker#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}